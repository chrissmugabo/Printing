generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Printer {
  id        Int     @id @default(autoincrement())
  type      String
  ip        String?
  port      String?
  interface String
  content   String

  @@map("printers")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String
  password String

  @@map("users")
}

model Setting {
  id          Int    @id @default(autoincrement())
  branch_id   Int
  branch_name String
  base_url    String

  @@map("settings")
}